{
  "name": "tree-sitter-verilog",
  "version": "0.11.0",
  "description": "Verilog grammar for tree-sitter",
  "license": "MIT",
  "author": "Aliaksei Chapyzhenka",
  "homepage": "https://github.com/tree-sitter/tree-sitter-verilog#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tree-sitter/tree-sitter-verilog.git"
  },
  "bugs": {
    "url": "https://github.com/tree-sitter/tree-sitter-verilog/issues"
  },
  "main": "index.js",
  "files": [
    "grammar.js",
    "binding.gyp",
    "src/*"
  ],
  "keywords": [
    "parser",
    "tree-sitter",
    "verilog"
  ],
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "gen": "tree-sitter generate",
    "install": "if [ ! -d ./src ]; then tree-sitter generate; fi && npm run get && node-gyp configure build",
    "test": "tree-sitter test",
    "get": "mkdir -p src/tree_sitter && wget https://raw.githubusercontent.com/tree-sitter/tree-sitter/master/include/tree_sitter/parser.h -O ./src/tree_sitter/parser.h"
  },
  "dependencies": {
    "nan": "^2.12.1"
  },
  "devDependencies": {
    "tree-sitter-cli": "0.14.0-beta3"
  }
}
